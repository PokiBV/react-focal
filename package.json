{
  "name": "react-focal",
  "version": "0.1.0",
  "description": "Component to help select the focal point in an image",
  "main": "lib/index.js",
  "author": "chielkunkels",
  "license": "MIT",
  "keywords": [
    "image",
    "focal",
    "focal",
    "point",
    "crop"
  ],
  "scripts": {
    "build:scripts": "babel -d lib src",
    "build:styles": "node-sass src/focal.scss lib/focal.css",
    "build": "npm-run-all clean --parallel build:*",
    "clean": "rimraf lib",
    "dev:scripts": "babel -w -d lib src",
    "dev:styles": "npm run build:styles && node-sass -w src/focal.scss lib/focal.css",
    "dev": "npm-run-all clean --parallel dev:*"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/chielkunkels/react-focal.git"
  },
  "bugs": {
    "url": "https://github.com/chielkunkels/react-focal/issues"
  },
  "homepage": "https://github.com/chielkunkels/react-focal#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.3.0",
    "rimraf": "^2.5.3"
  },
  "peerDependencies": {
    "react": "^15.2.0"
  }
}
